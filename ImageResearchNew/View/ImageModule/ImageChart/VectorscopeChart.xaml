<UserControl x:Class="ImageResearchNew.View.ImageModule.ImageChart.VectorscopeChart"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ImageResearchNew.View.ImageModule.ImageChart"
             xmlns:imagecharts="clr-namespace:ImageResearchNew.ViewModel.ImageModule.ImageCharts" xmlns:controls="clr-namespace:ImageResearchNew.View.ImageModule.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             d:DataContext="{d:DesignInstance imagecharts:VectorscopeChartViewModel}">
    <StackPanel>
        <controls:VectoroscopeChartControl XAsixText="{Binding XAxisText}" YAsixText="{Binding YAxisText}" Width="520" Height="440" Source="{Binding ModifiedImage}"/>
    </StackPanel>
</UserControl>
